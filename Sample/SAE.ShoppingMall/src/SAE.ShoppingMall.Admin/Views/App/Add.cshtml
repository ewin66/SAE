@section Js{
    <script src="~/content/js/app/add.js"></script> 
}
    <div class="col-md-6" data-bind="with:app">
        <form method="post" data-bind="attr:{action:url}" data-ajax="true">
            <div class="box box-danger">
                <div class="box-header">
                    <h3 class="box-title">App</h3>
                </div>
                <div class="box-body">
                    <sae-form-text params="object:id"></sae-form-text>
                    <sae-form-text params="object:secret"></sae-form-text>
                    <sae-form-text params="object:name"></sae-form-text>
                    <sae-form-text params="object:signin"></sae-form-text>
                    <sae-form-text params="object:signout"></sae-form-text>
                </div>
                <!-- /.box-body -->
            </div>
            <!-- /.box -->
            <button type="submit" class="btn btn-block btn-success">Success</button>
        </form>
    </div>